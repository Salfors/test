function Void() {
	case $TODO in 
		"1")
			snap_note= echo -e "\n${SM} Snap does not support your distro ${EM}\n"  ;;

		"2")
			
			sudo xbps-install -S flatpak 
			sudo flatpak remote-add --if-not-exists flathub https://flathub.org/repo/flathub.flatpakrepo ;;

		"3")
			snap_note= echo -e "\n${SM} Snap does not support your distro ${EM}\n"  
			sudo xbps-install -S flatpak 
			sudo flatpak remote-add --if-not-exists flathub https://flathub.org/repo/flathub.flatpakrepo ;;
	esac
}