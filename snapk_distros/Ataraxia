function Ataraxia() {
	
	case $TODO in 
		"1")
			function Ataraxia_snap() { 

				snap_note= echo -e "\n${SM} Snap does not support your distro ${EM}\n" 

			}
			Ataraxia_snap ;;

		"2")
			function Ataraxia_Flatpak() {

				echo "Install FlatAPk" 
				sudo neko em flatpak
				sudo flatpak remote-add --if-not-exists flathub https://flathub.org/repo/flathub.flatpakrepo 

			}
			Ataraxia_Flatpak ;;

		"3")
			Ataraxia_snap
			Ataraxia_Flatpak ;;
esac

}